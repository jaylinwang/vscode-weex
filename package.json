{
    "name": "weex",
    "displayName": "weex",
    "description": "vscode weex support",
    "keywords": [
        "weex", "vue"
    ],
    "version": "0.2.0",
    "publisher": "jaylinwang",
    "license": "SEE LICENSE IN LICENSE",
    "icon": "images/weex.png",
    "engines": {
        "vscode": "^1.0.0"
    },
    // "activationEvents": [
    //     "onLanguage:weex"
    // ],
    "categories": [
        "Languages",
        "Snippets"
    ],
    "galleryBanner": {
        "color": "#3B3738",
        "theme": "dark"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Microsoft/vscode-react-native"
    },
    "contributes": {
        "languages": [{
            "id": "weex",
            "aliases": [
                "weex"
            ],
            "extensions": [
                ".vue",
                ".we"
            ],
            "configuration": "./weex.configuration.json"
        }],
        "grammars": [{
            "language": "weex",
            "scopeName": "text.html.vue",
            "path": "./syntaxes/weex.tmLanguage"
        }],
        "snippets": [{
            "language": "weex",
            "path": "./snippets/html.json"
        }, {
            "language": "weex",
            "path": "./snippets/javascript.json"
        }]
    },
    "__metadata": {
        "id": "6eea747e-1a39-4672-b0ad-0d830e7b8e33",
        "publisherId": "4b350eb6-eca5-485a-85ab-3b82eba43aaf",
        "publisherDisplayName": "JaylinWang"
    }
}