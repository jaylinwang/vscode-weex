    "web": {
        "prefix": "web",
        "body": "<web src=\"$1\">$2</web>",
        "description": "weex - define web tag"
    },
    "wxc-tabbar": {
        "prefix": "wxc-tabbar",
        "body": "<wxc-tabbar tab-items=\"$1\">$2</wxc-tabbar>",
        "description": "weex - define wxc-tabbar tag"
    },
    "wxc-navpage": {
