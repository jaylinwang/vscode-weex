        "description": "weex - define a list tag"
    },
    "cell": {
        "prefix": "cell",
        "body": "<cell>$1</cell>",
        "description": "weex - define a cell tag"
    },
    "text": {
        "prefix": "text",
        "body": "<text>$1</text>",
        "description": "weex - define a text tag"
    },
