        "prefix": "image",
        "body": "<image src=\"$1\">$2</image>",
        "description": "weex - define a image tag"
    },
    "input": {
        "prefix": "input",
        "body": [
            "<input", 
            "   type=\"$1\"",
            "   autofocus=\"$2\"",
