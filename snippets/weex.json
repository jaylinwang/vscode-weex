{
    "we-element": {
        "prefix": "we-element",
        "body": "<we-element name=\"$1\">$2</we-element>",
        "description": "weex - define a we-element tag"
    },
    "content": {
        "prefix": "content",
        "body": "<content>$1</content>",
        "description": "weex - define a content tag"
    },
    "scroller": {
        "prefix": "scroller",
        "body": "<scroller>$1</scroller>",
        "description": "weex - define a scroller tag"
